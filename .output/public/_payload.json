6,"ogImage":327,"body":330,"_type":71,"_id":451,"_source":73,"_file":452,"_stem":453,"_extension":76,"sitemap":454,"head":455},"/portfolio/mastermay","Перенос и обслуживание mastermay.ru","Перенесли сайт на новую платформу. Разработали индивидуальный дизайн и перерисовали все проекты.","/img/portfolio/mastermay/card.jpg","/img/portfolio/mastermay/thumb.jpeg",2023,"Мастермай","mastermay.ru",[125,323,128,324],"Дизайн","Обслуживание","https://mastermay.ru/","Сайт для строительной компании Мастермай",{"component":13,"props":328},{"title":329,"description":316},"Перенос сайта mastermay.ru",{"type":18,"children":331,"toc":449},[332,337,345,354,359,430,435],{"type":21,"tag":135,"props":333,"children":336},{"alt":321,"className":334,"image":335},[138],"/img/portfolio/mastermay/mastermay.png",[],{"type":21,"tag":142,"props":338,"children":339},{"title":144},[340],{"type":21,"tag":44,"props":341,"children":342},{},[343],{"type":31,"value":344},"Компания Мастермай работает на рынке строительства деревянных домов с 2007 года. Огромный опыт мастеров является\nнеоспоримым преимуществом компании. Собственное производство дерева выделяет компанию среди конкурентов.",{"type":21,"tag":142,"props":346,"children":348},{"title":347},"Задача",[349],{"type":21,"tag":44,"props":350,"children":351},{},[352],{"type":31,"value":353},"Перенести сайт строительной компании Мастермай со старого движка на современный. Добавить функционал,\nобновить проекты, разработать правильную структуру.",{"type":21,"tag":135,"props":355,"children":358},{"alt":321,"className":356,"image":357},[138],"/img/portfolio/mastermay/image1.jpg",[],{"type":21,"tag":142,"props":360,"children":362},{"title":361},"Реализация",[363],{"type":21,"tag":161,"props":364,"children":365},{},[366,371,376,381,386,391,396,401,406,411,420,425],{"type":21,"tag":165,"props":367,"children":368},{},[369],{"type":31,"value":370},"Специалисты нашей компании собрали правильную структуру на новом сайте. Выделили ключевые разделы:\nКаталог, фоторепортажи, отзывы, о компании, услуги, статьи.",{"type":21,"tag":165,"props":372,"children":373},{},[374],{"type":31,"value":375},"Собрали сайт на современной системе управления, внедрили полную поддержку Seo тегов и настроили\nвнутреннюю перелинковку. ",{"type":21,"tag":165,"props":377,"children":378},{},[379],{"type":31,"value":380},"Перенесли данные со старого сайта, сохранили старые адреса страниц, на новые страницы и разделы сделали редиректы",{"type":21,"tag":165,"props":382,"children":383},{},[384],{"type":31,"value":385},"Перерисовали старые проекты. Заполнили все характеристики проектов.",{"type":21,"tag":165,"props":387,"children":388},{},[389],{"type":31,"value":390},"Настроили фильтр подбора проектов, теперь клиенты могут подобрать проекты по их запросам.",{"type":21,"tag":165,"props":392,"children":393},{},[394],{"type":31,"value":395},"Для фоторепортажей добавили блок с информацией и картой, на которой расположен проект. Также можно\nуказать по какому проекту построен дом.",{"type":21,"tag":165,"props":397,"children":398},{},[399],{"type":31,"value":400},"Добавили интерактивную карту всех построенных проектов.",{"type":21,"tag":165,"props":402,"children":403},{},[404],{"type":31,"value":405},"Внедрили страницу популярных вопросов и ответов на них.",{"type":21,"tag":165,"props":407,"children":408},{},[409],{"type":31,"value":410},"На странице проекта добавили новый расчёт цены проекта и новую комплектацию.",{"type":21,"tag":165,"props":412,"children":413},{},[414],{"type":21,"tag":415,"props":416,"children":417},"strong",{},[418],{"type":31,"value":419},"На сайт внедрена система расчета цен проекта по количеству материала. Каждый проект\nрассчитывается исходя из текущих цен на материалы. Актуальные цены в один клик.",{"type":21,"tag":165,"props":421,"children":422},{},[423],{"type":31,"value":424},"Также на сайте присутствует расчёт доставки, форма калькулятора расчёта дома, модуль избранных проектов, модуль\nлайков, поиск по сайту.",{"type":21,"tag":1